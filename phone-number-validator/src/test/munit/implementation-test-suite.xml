				</munit-tools:with-attributes>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to implementationFlow" doc:id="01ddb548-354c-45e2-b6cb-671c00767a7b" name="implementationFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="a137e424-e4b4-4583-bae2-d87e64931bbf" is="#[MunitTools::notNullValue()]"/>
		</munit:validation>
	</munit:test>


</mule>
